{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/users(.*)",
      "dest": "/api/v1/users$1"
    },
    {
      "src": "/api/v1/chats(.*)",
      "dest": "/api/v1/chats$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
